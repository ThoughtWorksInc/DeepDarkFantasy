name: DeepDarkFantasy
version: 0.0.1
cabal-version: 1.12
build-type: Simple
license: AllRightsReserved

executable DeepDarkFantasy
    main-is: Main.hs
    build-depends: base -any
    
    if impl(ghc < 7.10)
        build-depends: void -any

    default-language: Haskell2010
    hs-source-dirs: src
    ghc-options: -ferror-spans
